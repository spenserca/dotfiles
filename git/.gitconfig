[includeIf "gitdir:~/repos/work"]
  path = ~/.gitconfig-work

[alias]

  br = git branch
  ca = git commit -am
  cb = git checkout -b
  cm = git commit -m
  co = git checkout
  ga = git add
  gf = git fetch
  gh = git history
  pl = git pull
  ps = git push
  st = git status

[core]

  autocrlf = input

[user]

  email = spenserca@gmail.com
  name = Spenser Andreassen

[init]

  defaultBranch = main
